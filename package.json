{
  "name": "typedoc-theme-oxide",
  "version": "0.1.7",
  "description": "A TypeDoc theme looks just like rustdoc.",
  "repository": "github:balthild/typedoc-theme-oxide",
  "author": "Balthild <ibalthild@gmail.com>",
  "license": "MIT",
  "keywords": [
    "rustdoc",
    "typedoc",
    "typedoc-theme",
    "typescript"
  ],
  "scripts": {
    "download": "node scripts/download-assets.js",
    "watch": "rm -rf dist && tsc -w",
    "build": "rm -rf dist && tsc",
    "example:dev": "nodemon --watch dist --exec 'yarn run example:build'",
    "example:build": "cd example && typedoc --options ./typedoc.json",
    "example:serve": "http-server ./example/docs",
    "example:deploy": "gh-pages -d ./example/docs",
    "prepublishOnly": "yarn build"
  },
  "packageManager": "yarn@4.9.1",
  "main": "dist/index.js",
  "files": [
    "dist",
    "assets"
  ],
  "dependencies": {
    "cheerio": "^1.0.0-rc.11",
    "slug": "^10.0.0"
  },
  "devDependencies": {
    "@types/node": "^22.15.3",
    "@types/slug": "^5.0.9",
    "clean-css": "^5.3.0",
    "gh-pages": "^6.3.0",
    "http-server": "^14.1.1",
    "nodemon": "^3.1.10",
    "typedoc": "^0.28.4",
    "typescript": "^5.8.3",
    "undici": "^7.8.0"
  },
  "peerDependencies": {
    "typedoc": "^0.28.4"
  }
}
