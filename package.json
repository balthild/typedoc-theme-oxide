{
  "name": "typedoc-theme-oxide",
  "version": "0.2.0",
  "description": "A TypeDoc theme looks just like rustdoc.",
  "repository": "github:balthild/typedoc-theme-oxide",
  "author": "Balthild <ibalthild@gmail.com>",
  "license": "MIT",
  "keywords": [
    "rustdoc",
    "typedoc",
    "typedoc-theme",
    "typescript"
  ],
  "scripts": {
    "dev": "rollup -c -w",
    "build": "rollup -c",
    "clean": "rm -rf dist example/docs",
    "download": "node scripts/download-assets.js",
    "example:prepare": "cp -f ./node_modules/monaco-editor/esm/vs/editor/editor.api.d.ts ./example/",
    "example:build": "yarn example:prepare && cd example && typedoc --options ./typedoc.json",
    "example:deploy": "yarn build && yarn example:build && gh-pages -d ./example/docs",
    "release": "release-it"
  },
  "packageManager": "yarn@4.9.1",
  "type": "module",
  "main": "dist/plugin/index.js",
  "files": [
    "dist"
  ],
  "dependencies": {
    "cheerio": "^1.1.2",
    "flexsearch": "^0.8.212",
    "slug": "^11.0.1"
  },
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^16.0.3",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^12.3.0",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^22.15.17",
    "@types/slug": "^5.0.9",
    "clean-css": "^5.3.3",
    "gh-pages": "^6.3.0",
    "http-server": "^14.1.1",
    "lit": "^3.3.2",
    "lodash-es": "^4.17.23",
    "monaco-editor": "^0.55.1",
    "postcss": "^8.5.6",
    "postcss-nested": "^7.0.2",
    "release-it": "^19.2.4",
    "rollup": "^4.55.3",
    "rollup-plugin-postcss": "^4.0.2",
    "terser": "^5.46.0",
    "tslib": "^2.8.1",
    "typedoc": "^0.28.16",
    "typescript": "^5.9.3",
    "undici": "^7.19.0"
  },
  "peerDependencies": {
    "typedoc": "^0.28.4"
  }
}
