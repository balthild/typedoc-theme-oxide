{
  "name": "typedoc-theme-oxide",
  "version": "0.1.10",
  "description": "A TypeDoc theme looks just like rustdoc.",
  "repository": "github:balthild/typedoc-theme-oxide",
  "author": "Balthild <ibalthild@gmail.com>",
  "license": "MIT",
  "keywords": [
    "rustdoc",
    "typedoc",
    "typedoc-theme",
    "typescript"
  ],
  "scripts": {
    "dev": "rollup -c -w",
    "build": "rollup -c",
    "clean": "rm -rf dist example/docs",
    "download": "node scripts/download-assets.js",
    "example:build": "cd example && typedoc --options ./typedoc.json",
    "example:deploy": "gh-pages -d ./example/docs",
    "prepublishOnly": "yarn run clean && yarn run build"
  },
  "packageManager": "yarn@4.9.1",
  "type": "module",
  "main": "dist/plugin/index.js",
  "files": [
    "dist"
  ],
  "dependencies": {
    "cheerio": "^1.0.0-rc.11",
    "flexsearch": "^0.8.161",
    "slug": "^10.0.0"
  },
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^16.0.1",
    "@rollup/plugin-typescript": "^12.1.2",
    "@types/lodash-es": "^4",
    "@types/node": "^22.15.3",
    "@types/slug": "^5.0.9",
    "clean-css": "^5.3.0",
    "gh-pages": "^6.3.0",
    "http-server": "^14.1.1",
    "lit": "^3.3.0",
    "lodash-es": "^4.17.21",
    "postcss": "^8.5.3",
    "rollup": "^4.40.1",
    "rollup-plugin-postcss": "^4.0.2",
    "terser": "^5.39.0",
    "tslib": "^2.8.1",
    "typedoc": "^0.28.4",
    "typescript": "^5.8.3",
    "undici": "^7.8.0"
  },
  "peerDependencies": {
    "typedoc": "^0.28.4"
  },
  "resolutions": {
    "lunr@npm:^2.3.9": "patch:lunr@npm%3A2.3.9#~/.yarn/patches/lunr-npm-2.3.9-fa3aa9c2d6.patch"
  }
}
