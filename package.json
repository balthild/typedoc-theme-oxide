{
  "name": "typedoc-theme-oxide",
  "version": "0.1.7",
  "description": "A TypeDoc theme looks just like rustdoc.",
  "main": "dist/index.js",
  "repository": "github:balthild/typedoc-theme-oxide",
  "author": "Balthild <ibalthild@gmail.com>",
  "license": "MIT",
  "keywords": [
    "rustdoc",
    "typedoc",
    "typedoc-theme",
    "typescript"
  ],
  "scripts": {
    "dev": "watchexec --restart --watch src 'yarn run build && yarn run example'",
    "serve": "python3 -m http.server --directory ./example/docs",
    "retrieve": "node scripts/retrieve-assets.js",
    "build": "rm -rf dist && tsc",
    "example": "cd example && typedoc --options ./typedoc.json",
    "example-deploy": "gh-pages -d ./example/docs",
    "prepublishOnly": "yarn build"
  },
  "files": [
    "dist",
    "assets"
  ],
  "dependencies": {
    "cheerio": "^1.0.0-rc.11"
  },
  "devDependencies": {
    "@types/node": "^22.15.3",
    "clean-css": "^5.3.0",
    "gh-pages": "^6.3.0",
    "typedoc": "^0.28.4",
    "typescript": "^5.8.3",
    "undici": "^7.8.0"
  },
  "peerDependencies": {
    "typedoc": "^0.28.4"
  },
  "packageManager": "yarn@4.9.1"
}
